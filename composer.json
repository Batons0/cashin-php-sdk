{
    "name": "batons0/cashin-php-sdk",
    "description": "Directa24 PHP SDK",
    "type": "library",
    "homepage": "https://github.com/directa24",
    "license": "MIT",
    "keywords": [
        "directa24",
        "cashin"
    ],
    "authors": [
        {
            "name": "Directa24",
            "email": "support@directa24.com",
            "homepage": "https://directa24.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.6"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Directa24\\": "src/",
            "Directa24\\Directa24Test\\": "tests/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Directa24\\": "src/",
            "Directa24\\Directa24Test\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "replace": {
        "directa24/cashin-php-sdk": "*"
    }
}
